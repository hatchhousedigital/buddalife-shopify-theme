{%- assign headerSnippetPath = "header-index" -%}{%- assign wfPage = "61702d2b859f5422182d0712" -%}{%- assign bodyClasses = "" -%}
  {%- render 'navbar' -%}
  <div class="section about wf-section">
    <div class="container side-by-side">
      <div class="_30-percent-width-wrap">
        <div class="image-wrap">
          <div class="image content-cateogry-hero"></div>
        </div>
      </div>
      <div class="about-us">
        <h1 class="heading">{{ blog.title }}</h1>
        <p class="paragraph">{{ blog.metafields.global.description }}</p>
        <a href="#" class="button w-button">{{ 't2db00960' | t }}</a>
      </div>
    </div>
    <div class="absolute-side-background large-left about"></div>
    <div class="right-square">
      <div class="large-square about"></div>
    </div>
  </div>
  <div class="section wf-section">
    <div class="container">
      <div class="categories">
        <div class="categories-nav">
          <div class="sticky">
            <div class="scroll-to-see-wrapper"></div>
            <div class="content-categories-list-wrap">
              {%- for link in linklists["all-blogs"].links -%}<div class="content-categories-item">
                <a href="{{ link.url }}" class="button categories article-categories w-button">{{ link.title }}</a>
              </div>{%- endfor -%}{%- render 'menu-error', menu: "all-blogs" -%}
            </div>
          </div>
        </div>
        <div class="flex verticle">
          <div class="catergories-contain category-page">
            <h2 class="content-category-heading">{{ blog.title }}</h2>
            {%- render "body_aches_and_pains_archive" -%}
            <div class="articles-wrapper"></div>
          </div>
          <div class="catergories-contain category-page">
            <h2 class="content-category-heading">Products for {{ blog.title }}</h2>
            <div class="products content-category-page">
              {%- if blog.metafields.global.collection_handle != nil -%}
              {%- render "markup_for_custom_blog", chosen_collection_handle: blog.metafields.global.collection_handle -%}
              {%- else -%}
              {%- render "products_max_6_number_1" -%}
              {%- endif -%}
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
  {%- section 'footer' -%}
  <script src="https://d3e54v103j8qbb.cloudfront.net/js/jquery-3.5.1.min.dc5e7f18c8.js?site=61702d2b859f5470f02d0707" type="text/javascript" integrity="sha256-9/aliU8dGd2tb6OSsuzixeV4y/faTqgFtohetphbbj0=" crossorigin="anonymous"></script>
  <script src="{{ 'webflow.js' | asset_url }}" type="text/javascript"></script>
  <!-- [if lte IE 9]><script src="https://cdnjs.cloudflare.com/ajax/libs/placeholders/3.0.2/placeholders.min.js"></script><![endif] -->

{{ 'udesly-shopify.min.js' | asset_url | script_tag }}