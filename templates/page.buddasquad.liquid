{%- assign headerSnippetPath = "header-index" -%}{%- assign wfPage = "61702d2b859f5471872d0713" -%}{%- assign bodyClasses = "" -%}
  {%- render 'navbar' -%}
  <div class="section about wf-section">
    <div class="container side-by-side">
      <div class="_45-percent-width-wrap">
        <div class="image-wrap">
          {%- if page.metafields.global.featured_image == nil -%}<div class="image about-hero"></div>{%- else -%}<div class="image about-hero" style="background-image: url('{{ page.metafields.global.featured_image | img_url: 'master' }}');"></div>{%- endif -%}
        </div>
      </div>
      <div class="about-us">
        <h1 class="heading">{{ 't4e14c97b' | t }}</h1>
        <h4>{{ 'tn56cdb14f' | t }}</h4>
        <div class="w-richtext">{{ page.content }}</div>
        <a href="#" class="button w-button">{{ 't3a5cc311' | t }}</a>
      </div>
    </div>
    <div class="absolute-side-background large-left about"></div>
    <div class="right-square">
      <div class="large-square about"></div>
    </div>
  </div>
  <div class="section wf-section">
    <div class="container">
      <div class="w-layout-grid grid-2">
        {%- for link in linklists["buddasquad-team"].links -%}{%- assign object = link.object -%}<a href="{{ object.url }}" class="buddasquad-card w-inline-block">
          <div class="team-circle-img" style="background-image: url('{{ object.metafields.global.image | file_url }}');"></div>
          <h4>{{ object.title }}</h4>
          <p class="buddasquad-bio">{{ object.metafields.global.bio }}</p>
        </a>{%- endfor -%}{%- render 'menu-error', menu: "buddasquad-team" -%}
      </div>
    </div>
  </div>
  {%- section 'footer' -%}
  <script src="https://d3e54v103j8qbb.cloudfront.net/js/jquery-3.5.1.min.dc5e7f18c8.js?site=61702d2b859f5470f02d0707" type="text/javascript" integrity="sha256-9/aliU8dGd2tb6OSsuzixeV4y/faTqgFtohetphbbj0=" crossorigin="anonymous"></script>
  <script src="{{ 'webflow.js' | asset_url }}" type="text/javascript"></script>
  <!-- [if lte IE 9]><script src="https://cdnjs.cloudflare.com/ajax/libs/placeholders/3.0.2/placeholders.min.js"></script><![endif] -->

