{%- assign headerSnippetPath = "header-index" -%}{%- assign wfPage = "6172d150ccc5f50eba191b18" -%}{%- assign bodyClasses = "" -%}
  <div class="page-wrapper">
    {%- render 'navbar' -%}
    <div class="section article-post wf-section">
      <div class="scroll-indicator scroll-indicator-2"></div>
      <div class="blog-post-hero-image-wrap">
        <div class="blog-post-image" style="background-image: url('{{article.image | img_url: 'master'}}')"></div>
      </div>
      <div class="blog-content-contain">
        <div class="title-and-author">
          <h1 class="artcile-heading">{{ article.title }}</h1>
          <a href="#" class="author-card on-article w-inline-block">
            <div class="author-circle-img" style="background-image: url('{{ article.user.image | img_url: 'large' }}');"></div>
            <h5 class="author-name">{{ article.author }}</h5>
          </a>
        </div>
        <div class="flex space-between">
          <div class="article-date">{{ article.metafields.udesly['article-date'] | date: "%B %e, %Y" }}</div>
          <div class="flex">
            <div class="related-content">{{ 'tn5b80fd89' | t }}</div>
            <a href="/blogs/sustainability-living" class="related-content-category-link">{{ 'tn52c4e974' | t }}</a>
          </div>
        </div>
        <div class="rich-text-block w-richtext">{{ article.content }}</div>
      </div>
      <div class="right-square">
        <div class="large-square article-template"></div>
      </div>
    </div>
    {%- render 'categories-symbol' -%}
    {%- section 'join-cta' -%}
    {%- render 'popular' -%}
    {%- section 'footer' -%}
  </div>
