{%- assign headerSnippetPath = "header-index" -%}{%- assign wfPage = "61702d2b859f5471f12d071e" -%}{%- assign bodyClasses = "" -%}
  {%- render 'navbar' -%}
  <div class="section shop-categories wf-section">
    <div class="container">
      <div class="category-page-header w-row">
        <div class="category-brand-image-col w-col w-col-4"><img src="{{ collection.image | img_url: 'master' }}" loading="lazy" alt="{{ collection.image.alt }}" class="category-brand-image" srcset="{{ collection.image | img_url: '1920x' }} 1920w,{{ collection.image | img_url: '1600x' }} 1600w,{{ collection.image | img_url: '1366x' }} 1366w,{{ collection.image | img_url: '1024x' }} 1024w,{{ collection.image | img_url: '768x' }} 768w,{{ collection.image | img_url: '640x' }} 640w"></div>
        <div class="w-col w-col-8">
          <p>{{ collection.description }}</p>
        </div>
      </div>
      <div class="categories-nav">
        <div class="sticky">
          {%- render "categories_sorted_by_sort_order" -%}
        </div>
      </div>
      <div class="products-contain">
        {%- if settings.collection_categories_template != blank -%}{%- render "markup_for_collection_categories_template" -%}{%- else -%}{%- render "products_where_category_idin_current" -%}{%- endif -%}
        <div class="more-resources-contain">
          <div class="more-resources-wrapper">
            <h3 class="heading-4">{{ 't48133ed2' | t }}</h3>
          </div>
        </div>
      </div>
    </div>
    <div class="right-square categories-shop">
      <div class="large-square shop"></div>
    </div>
  </div>
  {%- section 'footer' -%}
  <script src="https://d3e54v103j8qbb.cloudfront.net/js/jquery-3.5.1.min.dc5e7f18c8.js?site=61702d2b859f5470f02d0707" type="text/javascript" integrity="sha256-9/aliU8dGd2tb6OSsuzixeV4y/faTqgFtohetphbbj0=" crossorigin="anonymous"></script>
  <script src="{{ 'webflow.js' | asset_url }}" type="text/javascript"></script>
  <!-- [if lte IE 9]><script src="https://cdnjs.cloudflare.com/ajax/libs/placeholders/3.0.2/placeholders.min.js"></script><![endif] -->

{{ 'udesly-shopify.min.js' | asset_url | script_tag }}