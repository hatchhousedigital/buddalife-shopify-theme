{%- assign headerSnippetPath = "header-index" -%}{%- assign wfPage = "61702d2b859f5471f12d071e" -%}{%- assign
bodyClasses = "" -%}
{%- render 'navbar' -%}
<style>
  .template-collection .new-banner .w-richtext * {
    color: #272d2c;
  }

  .template-collection .new-banner .w-richtext {
    filter: opacity(0.8);
  }

  .template-collection .new-banner .w-button {
    background-color: white;
    color: #1a927f;
    border-radius: 8px;
  }

  .template-collection .new-banner .image.collection-hero {
    width: 200px;
    height: auto;
    margin: auto;
    object-fit: contain;
    background-image: none;
  }

  .template-collection .new-banner .absolute-side-background {
    max-height: 65%;
    /*border-color: white;*/
    width: 35%;
    border-left: 0px;
    border-top: 0px;
  }

  .template-collection .new-banner .image-wrap {
    text-align: center;
    justify-content: center;
    align-items: center;
    display: flex;
    height: 100%;
  }

  .section.collection.new-banner {
    height: 80vh;
    background: linear-gradient(305deg, transparent, #fff 88%),
    url({{collection.metafields.my_fields.background_image | img_url: 'master'}}) center center no-repeat;
    background-size: cover;
  }

 

  .template-collection .new-banner .collection-hero-background {
    background-color: rgba(16, 142, 123, 1);
    padding: 20px;
    width: 230px;
    display: flex;
    height: 230px;
    justify-content: center;
    border-radius: 40px;
  }


  @media screen and (max-width: 991px) {
    .template-collection .new-banner .absolute-side-background {
      border: none;
    }


  }

  @media screen and (max-width: 479px) {
    .section.collection.new-banner {
      position: static;
      height: 100vh;
      padding: 0 20px;
    }

    .section.collection.wf-section.new-banner.buddapet {
       background-position: 36% 0px;
    }

    .section.collection.wf-section.new-banner.buddababy {
      background-position: 66%;
    }

    .template-collection .new-banner .w-richtext {
      font-size: 75%;
      text-align: center;
    }

    .template-collection .new-banner .collection-hero-background {
      width: 180px;
      height: 180px;
    }

    .template-collection .new-banner .image.collection-hero {
      width: 160px;
    }
  }
</style>
<div class="section collection wf-section new-banner {{collection.handle}}">
  <div class="container side-by-side">
    <div class="absolute-side-background large-left about"></div>
    <div class="_30-percent-width-wrap">
      <div class="image-wrap collection-hero">
        <div class="collection-hero-background">
          <img class="image collection-hero" src="{{collection.metafields.my_fields.logo_white | img_url: 'master'}}" />
        </div>
      </div>
    </div>
    <div class="about-us">
      {%- if collection.metafields.global.hide-title == true -%}<h1 class="heading" style="display:none">{{
        collection.title }}</h1>{%- else -%}<h1 class="heading">{{ collection.title }}</h1>{%- endif -%}
      <div class="w-richtext">{{ collection.description }}</div>
      <a href="#shop" class="button w-button">Shop Now</a>
    </div>
  </div>
  <div class="right-square categories-shop">
    <div class="large-square shop"></div>
  </div>
</div>
<div class="section wf-section">
  <div class="container">
    <div class="products-contain" id="shop">
      {%- if settings.collection_categories_template != blank -%}{%- render
      "markup_for_collection_categories_template" -%}{%- else -%}{%- render "products_where_category_idin_current"
      -%}{%- endif -%}
      <div class="more-resources-contain">
        <div class="more-resources-wrapper">
          <h3 class="heading-4">{{ 't48133ed2' | t }}</h3>
          {%- render "articles_from_variable_linklistscollectionhandlelinks" -%}
        </div>
      </div>
    </div>
  </div>
</div>
{%- section 'footer' -%}
<script src="https://d3e54v103j8qbb.cloudfront.net/js/jquery-3.5.1.min.dc5e7f18c8.js?site=61702d2b859f5470f02d0707"
  type="text/javascript" integrity="sha256-9/aliU8dGd2tb6OSsuzixeV4y/faTqgFtohetphbbj0="
  crossorigin="anonymous"></script>
<script src="{{ 'webflow.js' | asset_url }}" type="text/javascript"></script>
