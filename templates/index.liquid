{%- assign headerSnippetPath = "header-index" -%}{%- assign wfPage = "61702d2b859f54d4af2d0708" -%}{%- assign bodyClasses = "" -%}
  <div class="page-wrapper">
    {%- render 'navbar' -%}
    <div class="section home-hero wf-section">
      <div class="container side-by-side home-hero">
        <div class="_45-percent-width-wrap">
          <div class="image-wrap">
            <div class="image home-hero"></div>
          </div>
        </div>
        <div class="about-us">
          <h1 class="heading">{{ 'tn516a9018' | t }}<br>{{ 'tn2b8b8ec9' | t }}<br>{{ 't7b4add44' | t }}</h1>
          <h4>{{ 't6b26d60c' | t }}</h4>
          <p class="paragraph">{{ 't3cbbf807' | t }}</p>
          <a href="{{ pages["about-us"].url }}" class="button w-button">{{ 'tn52bd880f' | t }}</a>
        </div>
      </div>
      <div class="right-square">
        <div class="large-square about"></div>
      </div>
    </div>
    <div class="section home wf-section">
      <div class="container">
        <div data-delay="4000" data-animation="cross" class="slider w-slider" data-autoplay="true" data-easing="ease" data-hide-arrows="false" data-disable-swipe="false" data-autoplay-limit="0" data-nav-spacing="5" data-duration="800" data-infinite="true">
          <div class="w-slider-mask">
            <div data-w-id="08bff151-c556-3a92-7d24-23e6bb3e4670" class="w-slide">
              <div class="slider-content">
                {%- render "sustainable_living_where_hero_number_eq_01" -%}
              </div>
            </div>
            <div data-w-id="08bff151-c556-3a92-7d24-23e6bb3e4671" class="w-slide">
              <div class="slider-content">
                {%- render "special_needs" -%}
              </div>
            </div>
            <div data-w-id="d844198f-b468-dbe5-3b1f-6bb42b2038d4" class="w-slide">
              <div class="slider-content">
                {%- render "sleeps" -%}
              </div>
            </div>
            <div data-w-id="28ffe8e6-6c7a-8b3a-8738-befc4874e5cd" class="w-slide">
              <div class="slider-content">
                {%- render "pregnancies" -%}
              </div>
            </div>
          </div>
          <div class="left-arrow w-slider-arrow-left"><img src="{{ 'bottom-slider-arrow.svg' | asset_url }}" loading="lazy" width="28" alt="" class="bottom-arrow"></div>
          <div class="right-arrow w-slider-arrow-right"><img src="{{ 'top-slider-arrow.svg' | asset_url }}" loading="lazy" width="28" alt="" class="top-arrow"></div>
          <div class="slide-nav w-slider-nav w-round"></div>
        </div>
      </div>
      <div class="teal-dot"></div>
      <div class="teal-dot"></div>
      <div class="teal-dot"></div>
      <div class="absolute-side-background home"></div>
      <div class="dot"></div>
    </div>
    {%- render 'categories-symbol' -%}
    {%- section 'join-cta' -%}
    <div class="section wf-section">
      <div class="container">
        <div class="title-wrap align-center">
          <h2 class="title">{{ 't6078f32d' | t }}<span class="e2a57b"></span><span class="e2a57b"></span></h2>
          <div>
            <p class="paragraph">{{ 'tn546e3839' | t }}</p>
          </div>
          <div class="w-layout-grid grid-3">
            {%- for link in linklists["all-blogs"].links -%}{%- assign object = link.object -%}<a href="{{ object.url }}" class="content-categories-card w-inline-block">
              <div class="content-category" style="background-image: url('{{ object.metafields.global.image | file_url }}');"></div>
              <h5 class="category-description">{{ object.title }}</h5>
            </a>{%- endfor -%}{%- render 'menu-error', menu: "all-blogs" -%}
          </div>
        </div>
      </div>
    </div>
    {%- section 'shop-cta' -%}
    {%- render 'recent-articles' -%}
    {%- section 'footer' -%}
  </div>
  <script src="https://d3e54v103j8qbb.cloudfront.net/js/jquery-3.5.1.min.dc5e7f18c8.js?site=61702d2b859f5470f02d0707" type="text/javascript" integrity="sha256-9/aliU8dGd2tb6OSsuzixeV4y/faTqgFtohetphbbj0=" crossorigin="anonymous"></script>
  <script src="{{ 'webflow.js' | asset_url }}" type="text/javascript"></script>
