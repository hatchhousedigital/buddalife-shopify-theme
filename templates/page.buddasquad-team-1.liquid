{%- assign headerSnippetPath = "header-index" -%}{%- assign wfPage = "6171836a95140645709cb3ab" -%}{%- assign bodyClasses = "" -%}
  <div class="page-wrapper">
    {%- render 'navbar' -%}
    <div class="section author wf-section">
      <div class="container author">
        <div class="author-content-contain">
          <h1>{{ 'tn599899b4' | t }}</h1>
          <div class="w-richtext">{{ page.content }}</div>
        </div>
        <div class="sticky-nav">
          <div class="author-card">
            <div class="author-circle-img" style="background-image: url('{{ page.metafields.global.image | file_url }}');"></div>
            <h4 class="align-center">{{ page.title }}</h4>
            <p>{{ page.metafields.global.bio }}</p>
            <div class="social-links-wrapper">
              <a href="{{ page.metafields.global.linkedin }}" target="_blank" class="social-link w-inline-block"><img src="{{ 'linkedin-icon-png-transparent-background-5.png' | asset_url }}" width="18" sizes="(max-width: 767px) 18px, (max-width: 991px) 2vw, (max-width: 1279px) 18px, 1vw" srcset="{{ 'linkedin-icon-png-transparent-background-5-p-500.png' | asset_url }} 500w, {{ 'linkedin-icon-png-transparent-background-5.png' | asset_url }} 609w" alt=""></a>
              <a href="{{ page.metafields.global.instagram }}" target="_blank" class="social-link w-inline-block"><img src="{{ '174855.png' | asset_url }}" width="18" sizes="(max-width: 767px) 18px, (max-width: 991px) 2vw, (max-width: 1279px) 18px, 1vw" srcset="{{ '174855-p-500.png' | asset_url }} 500w, {{ '174855.png' | asset_url }} 512w" alt=""></a>
              <a href="{{ page.metafields.global.twitter }}" target="_blank" class="social-link w-inline-block"><img src="{{ 'twitter1.png' | asset_url }}" width="18" sizes="(max-width: 767px) 18px, (max-width: 991px) 2vw, (max-width: 1279px) 18px, 1vw" srcset="{{ 'twitter1-p-500.png' | asset_url }} 500w, {{ 'twitter1-p-800.png' | asset_url }} 800w, {{ 'twitter1.png' | asset_url }} 827w" alt=""></a>
            </div>
          </div>
        </div>
      </div>
      <div class="right-square">
        <div class="large-square author-page"></div>
      </div>
    </div>
    {%- section 'footer' -%}
  </div>
  <script src="https://d3e54v103j8qbb.cloudfront.net/js/jquery-3.5.1.min.dc5e7f18c8.js?site=61702d2b859f5470f02d0707" type="text/javascript" integrity="sha256-9/aliU8dGd2tb6OSsuzixeV4y/faTqgFtohetphbbj0=" crossorigin="anonymous"></script>
  <script src="{{ 'webflow.js' | asset_url }}" type="text/javascript"></script>
  <!-- [if lte IE 9]><script src="https://cdnjs.cloudflare.com/ajax/libs/placeholders/3.0.2/placeholders.min.js"></script><![endif] -->

